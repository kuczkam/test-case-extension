{
    "manifest_version": 2,
    "name": "Test Cases By Click",
    "description": "Create TC by click on the element",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "storage",
        "*://*/*"
    ]
}